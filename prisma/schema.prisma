  maxPeople     Int?     @db.TinyInt
  type          Int      @default(1) @db.TinyInt
  hasInterview  Boolean? @default(false)
  hasResume     Boolean? @default(false)
  resumeLink    String?  @db.Text
  interviewInfo String?  @db.Text

  @@index([leader], map: "leader")
  @@index([subleader], map: "subleader")
  @@index([type], map: "type")
}

