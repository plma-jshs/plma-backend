  provider = "mysql"
}

model IDBIapply {
  id         Int      @id @default(autoincrement()) @db.UnsignedInt
  clubId     Int      @db.TinyInt
  userId     Int      @db.UnsignedMediumInt
  createdAt  DateTime @db.DateTime(0)
  updatedAt  DateTime @db.DateTime(0)
  isApproved Boolean  @default(false)

  @@unique([clubId, userId], map: "IDBIapply_userID_clubID_unique")
  @@index([userId], map: "userID")
}

model IDBIbooths {
  id        Int       @id @default(autoincrement()) @db.SmallInt
  name      String    @db.Text
  operator  Int       @unique(map: "IDBIbooths_UNIQUE") @db.UnsignedMediumInt
  IDBIusers IDBIusers @relation(fields: [operator], references: [id], onDelete: Cascade, map: "IDBIbooths_IDBIusers_FK")
}

